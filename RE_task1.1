i\":7, \"viii\":8, \"ix\":9, \"x\":10\n}\n\ndef normalize_suffix(s):\n    \"\"\"\n    Convert last suffix to a sortable number.\n    Types supported:\n        - 3   (integer)\n        - a   (letter)\n        - (i) (roman numeral)\n    Returns None if the suffix is not logically sortable.\n    \"\"\"\n    s = s.lower().strip(\"() \")\n\n    # integer\n    if s.isdigit():\n        return int(s)\n\n    # alphabet (a → 1, b → 2...)\n    if len(s) == 1 and s.isalpha():\n        return ord(s) - ord(\"a\") + 1\n\n    # roman numerals\n    if s in roman_map:\n        return roman_map[s]\n\n    return None  # unsupported suffix\n\n\n# ----------------------------\n# GROUP CHILDREN BY PARENT\n# ----------------------------\n\ndef get_parent(num):\n    parts = num.split(\".\")\n    return \".\".join(parts[:-1]) if len(parts) > 1 else None\n\nchildren = defaultdict(list)\nfor h in headings:\n    parent = get_parent(h[\"numeric\"])\n    if parent:\n        children[parent].append(h[\"numeric\"])\n\n\n# ----------------------------\n# DETECT MISSING SIBLINGS\n# ----------------------------\n\nmissing_nodes = []\n\nfor parent, nums in children.items():\n\n    suffix_values = []\n    original_map = {}\n\n    for n in nums:\n        last = n.split(\".\")[-1]\n        val = normalize_suffix(last)\n        if val is not None:\n            suffix_values.append(val)\n            original_map[val] = n\n\n    if not suffix_values:\n        continue\n\n    suffix_values = sorted(set(suffix_values))\n    max_suffix = max(suffix_values)\n\n    # find missing\n    for i in range(1, max_suffix + 1):\n        if i not in suffix_values:\n            # reconstruct missing topic num\n            missing_nodes.append(f\"{parent}.{i}\")\n\nprint(\"Missing sibling topics:\")\nmissing_nodes\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-12T14:45:00.351825Z","iopub.execute_input":"2025-12-12T14:45:00.352062Z","iopub.status.idle":"2025-12-12T14:45:00.375302Z","shell.execute_repl