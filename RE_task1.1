 be only punctuation\n    if is_only_punctuation(s):\n        return False\n\n    # ---- Title must contain alphabetic characters\n    if not any(c.isalpha() for c in s):\n        return False\n\n    # ---- Too short\n    if len(s) < 3:\n        return False\n\n    # ---- ALL CAPS allowed\n    alpha_chars = sum(c.isalpha() for c in s)\n    if s.isupper() and alpha_chars >= 3:\n        return True\n\n    # ---- First meaningful token must be uppercase & not a stopword\n    first = s.split()[0]\n    if first.lower() in STOPWORDS:\n        return False\n    if not first[0].isupper():\n        return False\n\n    return True\n\n\ndef extract_headings(paras):\n    headings = []\n    last_major_topic = 0     # e.g., 1, 2, 3, 4...\n    last_numeric_root = None # e.g., 6 when parsing 6.1, 6.2\n\n    for p in paras:\n        text = (p.get(\"text\",\"\") or \"\").strip()\n        if not text or len(text) < 3:\n            continue\n\n        # reject \"1\" \"(1)\" \"3\"\n        if BAD_HEADING_RE.match(text):\n            continue\n\n        # ----- Rule A: TOPIC N matches -----\n        m = TOPIC_RE.match(text)\n        if m:\n            num = int(m.group(\"num\"))\n            title = m.group(\"title\").strip()\n\n            last_major_topic = num\n            last_numeric_root = num\n\n            if looks_like_title(title):\n                headings.append({\n                    \"para_idx\": p[\"para_idx\"],\n                    \"numeric\": str(num),\n                    \"title\": title\n                })\n            continue\n\n        # ----- Rule B: Numeric subtopics -----\n        m = NUMERIC_RE.match(text)\n        if not m:\n            continue\n\n        num_str = m.group(\"num\")\n        title = m.group(\"title\").strip()\n\n        # Always reject non-title garbage\n        if not looks_like_title(title):\n            continue\n\n        # Split numeric\n        parts = num_str.split(\".\")\n        major = int(parts[0])\n\n        # ====== NUMERIC ORDER VALIDATION ======\n        # Case 1: This is a new TOPIC root (e.g., 7)\n        if len(parts) == 1:\n            # Accept only if it is within reasonable range:\n            # e.g., next topic or same or small jump (< 3)\n            if abs(major - last_major_topic) > 2:\n                # Reject huge jumps like 30 after 6.3\n                continue\n\n            last_major_topic = major\n            last_numeric_root = major\n\n            headings.append({\n                \"para_idx\": p[\"para_idx\"],\n                \"numeric\": num_str,\n                \"title\": title\n            })\n            continue\n\n        # Case 2: subtopic like 6.1, 6.2, 6.3\n        if len(parts) >= 2:\n            # Validate major topic consistency\n            if last_numeric_root is not None and abs(major - last_numeric_root) > 0:\n                # Wrong topic lineage (e.g., inside Topic 6 block we suddenly get 30.x)\n                continue\n\n            last_numeric_root = major\n\n            headings.append({\n                \"para_idx\": p[\"para_idx\"],\n                \"numeric\": num_str,\n                \"title\": title\n            })\n            continue\n\n    return headings\n\n\n\nheadings = extract_headings(paras)\n\nprint(\"Detected headings:\", len(headings))\nheadings\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-12T14:45:00.327925Z","iopub.execute_input":"2025-12-12T14:45:00.328450Z","iopub.status.idle":"2025-12-12T14:45:00.351037Z","shell.execute_reply.started":"2025-12-12T14:45:00.328430Z","shell.execute_reply":"2025-12-12T14:45:00.350336Z"}},"outputs":[{"name":"stdout","text":"Detected headings: 129\n","output_type":"stream"},{"execution_count":5,"output_type":"execute_result","data":{"text/plain":"[{'para_idx': 5, 'numeric': '1', 'title': 'DEFINITIONS AND SCOPE'},\n {'para_idx': 6, 'numeric': '1.1', 'title': 'Purpose'},\n {'para_idx': 10, 'numeric': '1.2', 'title': 'Definitions'},\n {'para_idx': 25, 'numeric': '1.3'