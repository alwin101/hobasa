rence this topic or its subtopics (incoming cross-refs)\n    \"\"\"\n    graph = pydot.Dot(\n        f\"Topic_{topic}\",\n        graph_type=\"digraph\",\n        rankdir=\"TB\",\n        label=f\"<<B>Topic {topic} — Cross-Reference Map</B>>\",\n        labelloc=\"t\",\n        fontsize=\"14\"\n    )\n    \n    node_info = topic_map.get(topic, {})\n    title = node_info.get(\"title\", \"Unknown\")\n    children = node_info.get(\"children\", [])\n    \n    # Central topic node (highlighted)\n    central_node = pydot.Node(\n        topic,\n        label=f\"<<B>{topic}</B><BR/><I>{title}</I>>\",\n        shape=\"box\",\n        style=\"rounded,filled\",\n        fillcolor=\"#ffeb99\",\n        color=\"#ff6b35\",\n        penwidth=\"3\",\n        fontsize=\"12\"\n    )\n    graph.add_node(central_node)\n    \n    # Collect all topics in this main topic's scope (topic + children)\n    scope_topics = [topic] + children\n    \n    # Track which nodes we've added\n    added_nodes = {topic}\n    \n    # Add children nodes\n    for child in children:\n        child_info = topic_map.get(child, {})\n        child_node = pydot.Node(\n            child,\n            label=f\"<<B>{child}</B><BR/>{child_info.get('title', '')}>\",\n            shape=\"box\",\n            style=\"rounded,filled\",\n            fillcolor=\"#d4f1d4\",\n            fontsize=\"10\"\n        )\n        graph.add_node(child_node)\n        added_nodes.add(child)\n        \n        # Hierarchical edge (parent to child)\n        edge = pydot.Edge(topic, child, color=\"#009900\", penwidth=\"2\", arrowsize=\"0.8\")\n        graph.add_edge(edge)\n    \n    # ========================================\n    # OUTGOING REFERENCES (from topic + subtopics)\n    # ========================================\n    for source_topic in scope_topics:\n        source_info = topic_map.get(source_topic, {})\n        \n        # Use 'cross_refs' from topic_map (direct references only)\n        refs = source_info.get(\"cross_refs\",